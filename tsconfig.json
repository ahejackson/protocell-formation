{
  "compilerOptions": {
    "noEmit": true,
    "strictNullChecks": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ES6",
    "moduleResolution": "node"
  },
  "include": ["src"]
}
